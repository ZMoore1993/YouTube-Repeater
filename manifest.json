{
	"name": "Repeater",
	"version": "1.0",
	"description": "Automatically starts and repeats YouTube videos with a given URL.",
	"permissions": ["storage"],
	"browser_action": {
		"default_icon": {
			"16":"images/icon16.png",
			"24":"images/icon24.png",
			"32":"images/icon32.png"
		},
		"default_popup": "popup.html",
		"default_title": "YouTube Repeater"
	},
	"icons": {
		"16":"images/icon16.png",
		"24":"images/icon24.png",
		"32":"images/icon32.png"
	},
	"manifest_version": 2
}